super + e; {e,d,f}
      {emacsclient -c -a \'emacs\',discord,firefox}

super + p; {space,Right,Left,p}
      {mocp --toggle-pause,mocp -f,mocp -r,$HOME/.scripts/dm-mocp}

super + s; {f,s,c}
      flameshot {full -c -p $HOME/Screenshot,flameshot screen -c -p $HOME/Screenshots,flameshot gui -p $HOME/Screenshots}
